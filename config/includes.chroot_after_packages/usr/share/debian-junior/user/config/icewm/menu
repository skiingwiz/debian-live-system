separator
menu "Audio Video" folder {
  prog "VLC Media Player" vlc icewm-menu-fdo "/usr/share/applications/vlc.desktop"
  menu "Mixer" folder {
    prog "PulseAudio-Lautstärkeregler" multimedia-volume-control pavucontrol
    prog "PulseAudio Volume Meter (Capture)" audio-input-microphone pavumeter --record
    prog "PulseAudio Volume Meter (Playback)" audio-input-microphone pavumeter
  }
}

